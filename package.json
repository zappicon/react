{
  "name": "@zappicon/react",
  "version": "1.2.1",
  "description": "Free & premium UI icons library, crafted to simplify workflows for designers, developers and creators.",
  "main": "dist/index.cjs.js",
  "browser": "dist/index.es.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs.js"
    },
    "./icons/*": {
      "import": "./dist/icons/*.es.js",
      "require": "./dist/index.cjs.js"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "keywords": [
    "icons",
    "react",
    "svg",
    "components",
    "zappicon"
  ],
  "author": "Haroun Abidi (Developed under Zappicon) <contact@harounabidi.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zappicon/react.git"
  },
  "bugs": {
    "url": "https://github.com/zappicon/react/issues"
  },
  "homepage": "https://zappicon.com",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky",
    "clean": "rimraf dist && rimraf src/icons && rimraf src/variants",
    "generate": "npm run clean && tsx scripts/generate.ts",
    "build": "npm run generate && vite build",
    "test": "vitest --watch=false",
    "test:coverage": "vitest --coverage --watch=false",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.33.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^24.3.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.1.1",
    "eslint": "^9.33.0",
    "eslint-plugin-react": "^7.37.5",
    "glob": "^11.0.3",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "jiti": "^2.5.1",
    "jsdom": "^27.2.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "rimraf": "^6.0.1",
    "tslib": "^2.8.1",
    "tsx": "^4.20.4",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.1",
    "vite": "^7.2.2",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^4.0.8",
    "vitest-browser-react": "^2.0.2"
  }
}
